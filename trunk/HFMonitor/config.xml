	  <Input>L0.HBG_75</Input>
	  <Input>L0.DCF_77</Input>
	  <Input>L0.GXX_60</Input>
	</Inputs>
	<Name>CAL</Name>
      </Calibrator>
    </L1>
    <L2>
      <FindPeak>
	<Filter> 
	  <Type>None</Type>
	  <TimeConstant>60.0</TimeConstant>
	</Filter>
	<fMin>59.8e3</fMin>
	<fMax>60.2e3</fMax>
