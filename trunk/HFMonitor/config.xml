<?xml version="1.0" encoding="utf-8"?>
<!-- $Id$
HFMon config file:

server.data.port ..........
server.ctrl.port ..........
perseus.qrg_Hz ............ DDS frequency / Hz
perseus.fpga.samplerate ... samplingrate / Hz
Repack.BufferLengthSec .... buffer length in seconds for all following processors
Dump.SamplesPerFile ....... # samples / file
Dump.FileNamePattern ...... filename pattern for output
FFT.WindowFunction ........
  -->
<server>
  <debug filename="debug.log" level="2"></debug>
  <data port="18000"
	maxQueueDelay_Minutes="5" 
	maxQueueSize_MB="100"></data>
  <ctrl port="18001"></ctrl>
  <perseus samplerate="500000"
	   qrg_Hz    ="250000"
	   attenuator  ="0"
	   enableDither="1"
	   enablePreamp="0"	 
	   enablePresel="0"	 
	   USBBufferSize="16320">
    <CascadedPTimeLowPass>
      <Tau_Sec>600.</Tau_Sec>
      <Tau_Sec>60.</Tau_Sec>
    </CascadedPTimeLowPass>    
  </perseus>
</server>
<Repack>
  <BufferLengthSec>0.5</BufferLengthSec>
  <OverlapPercent>-100</OverlapPercent>
</Repack>
<FFT>
  <WindowFunction>Blackman</WindowFunction>
  <DataPath>Data</DataPath>
  <Actions>
    <L0>      
      <FindPeak>
	<Filter> 
	  <Type>LowPass</Type>
	  <TimeConstant>60.0</TimeConstant>
	</Filter>
	<fMin>77.3e3</fMin>
	<fMax>77.7e3</fMax>
	<fRef>77.5e3</fRef>
	<minRatio>10</minRatio>
