      </AverageDensity>
      <AverageDensity name="025200_Navy_NML"
		      fRef_Hz="25.200e3"
		      bandwidth_Hz="200">
	<Calibration key="L1.CAL"></Calibration>
      </AverageDensity>
      <AverageDensity name="037500_Navy_Grindavik"
		      fRef_Hz="37.5e3"
		      bandwidth_Hz="200">
