<FFTProcessor windowFunction="Blackman"
	      numberOfCollectedEpochs="1">
  <server host="127.0.0.1"
	  port="18001">
  </server>
  <Broadcaster>
    <Data port="8080"
	  maxQueueSize_MB="1"
	  maxQueueDelay_Minutes="2">
    </Data>
  </Broadcaster>
  <Repack bufferLength_sec="1"
	  overlap_percent="0">
  </Repack>
  <FileSink path="DataNTZ">
  </FileSink>
  <Actions calibrationKey="L1.CAL">
    <L0>
      <FindPeak name="060000_TS_WWVB"
		fMin_Hz="59.800e3"
		fMax_Hz="60.200e3"
		fRef_Hz="60.000e3"
		minRatio="10">
	<Filter type="LowPass" timeConstant_sec="60.0"></Filter>
      </FindPeak>
    </L0>
    <L1>
      <Calibrator name="CAL"
		  maxOffset_Hz="10.0"
		  maxCorrectionFactor_ppm="20.0">
	<Inputs>
	  <Input key="L0.060000_TS_WWVB"></Input>
	</Inputs>
      </Calibrator>
    </L1>
    <L2>
      <AverageDensity name="024000_Navy_NAA"
		      fRef_Hz="24.000e3"
		      bandwidth_Hz="200">
